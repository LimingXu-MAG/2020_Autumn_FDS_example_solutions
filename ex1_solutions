   "source": [
    "import numpy as np\n",
    "import scipy.optimize\n",
    "import scipy.special\n",
    "\n",
    "\n",
    "def logPr(x, τ):\n",
    "    λ = np.exp(τ)\n",
    "    lik = np.power(λ, x) / (np.exp(λ) * scipy.special.factorial(x))\n",
    "    return np.log(lik)\n",
    "\n",
    "x = [3, 2, 8, 1, 5, 0, 8]\n",
    "τ_hat = scipy.optimize.fmin(lambda τ: -np.sum(logPr(x, τ)), 0)\n",
    "\n",
    "λ_hat = np.log(τ_hat)\n",
    "\n",
    "print(λ_hat)"
   ]
